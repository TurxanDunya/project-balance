.unity-progress-bar__container {
    min-height: 135px;
    height: 135px;
    flex-grow: 1;
}

.unity-progress-bar__background {
    border-width: 5px;
    border-radius: 16px;
    border-color: rgb(255, 255, 255);
    height: 130px;
}

.unity-progress-bar__progress {
    height: 115px;
    border-radius: 16px;
    margin-left: 5px;
    margin-right: 2px;
    margin-top: 6px;
    margin-bottom: 7px;
    background-color: rgb(52, 235, 52);
    margin: 3px;
}

